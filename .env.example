# Riot Games API Key (regenerate every 24 hours)
# Get your key from: https://developer.riotgames.com/
# Rate Limits: 20 requests/second, 100 requests/2 minutes
RIOT_API_KEY=your_riot_api_key_here

# Apex Legends API Key (mozambiquehe.re)
# Get your key from: https://apexlegendsapi.com/
# Rate Limits: 1 request per 2 seconds (base), 2 requests per 2 seconds with Discord
APEX_API_KEY=your_apex_api_key_here

# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# CORS Settings (for local development)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
